{
  "name": "finance-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:seed": "npx tsx ./prisma/seed.ts",
    "db:migrate": "npx prisma migrate dev",
    "db:reset": "npx prisma migrate reset --force && npm run db:migrate && npm run db:seed",
    "db:reset2": "rm -rf prisma/migrations && npx prisma migrate reset --force && npm run db:migrate -- --name init && npm run db:seed",
    "import:ticker": "npx tsx ./src/commands/ImportTickerDataCommand.ts",
    "import:b3": "npx tsx ./src/commands/ImportB3FilesCommand.ts",
    "wallet:create": "npx tsx ./src/commands/WalletCreateCommand.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "cheerio": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.475.0",
    "next": "15.1.7",
    "object-hash": "^3.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "read-excel-file": "^5.8.6",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/lodash": "^4.17.15",
    "@types/node": "^20",
    "@types/object-hash": "^3.0.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8",
    "prisma": "^6.3.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
